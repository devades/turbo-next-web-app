{
  "name": "api-blog",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.cjs\"",
    "build": "tsup && pnpm dev:seed:db",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/ --max-warnings 0",
    "test": "jest --detectOpenHandles",
    "dev:seed:db": "rm -rf ./prisma/migrations && rm -rf ./prisma/db && prisma generate && prisma migrate dev --name init"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "jest": {
    "preset": "@devades/jest-presets/node"
  },
  "dependencies": {
    "@devades/logger": "0.0.2",
    "@prisma/client": "6.6.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "4.21.2",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@devades/eslint-config": "0.0.3",
    "@devades/jest-presets": "0.0.2",
    "@devades/typescript-config": "0.0.5",
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "prisma": "^6.6.0",
    "supertest": "^7.1.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "5.8.2"
  }
}
